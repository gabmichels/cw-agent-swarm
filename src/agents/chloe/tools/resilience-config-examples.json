{
  "fallbackChains": [
    {
      "primaryToolName": "webSearch",
      "fallbacks": [
        {
          "toolName": "bingSearch",
          "forFailureTypes": ["rate_limit_error", "timeout_error"],
          "priority": 10
        },
        {
          "toolName": "googleCustomSearch",
          "forFailureTypes": ["rate_limit_error", "timeout_error", "permission_error"],
          "priority": 8
        },
        {
          "toolName": "duckDuckGoSearch",
          "priority": 5
        }
      ]
    },
    {
      "primaryToolName": "imageGenerator",
      "fallbacks": [
        {
          "toolName": "alternativeImageGenerator",
          "forFailureTypes": ["timeout_error", "resource_error"],
          "priority": 10
        },
        {
          "toolName": "basicImageGenerator",
          "forFailureTypes": ["validation_error"],
          "priority": 8
        }
      ]
    },
    {
      "primaryToolName": "weatherApi",
      "fallbacks": [
        {
          "toolName": "alternativeWeatherApi",
          "forFailureTypes": ["rate_limit_error", "timeout_error"],
          "priority": 10
        },
        {
          "toolName": "weatherScraper",
          "priority": 5
        }
      ]
    },
    {
      "primaryToolName": "codeInterpreter",
      "fallbacks": [
        {
          "toolName": "sandboxedCodeExecutor",
          "forFailureTypes": ["timeout_error", "resource_error"],
          "priority": 10
        },
        {
          "toolName": "simplifiedCodeExecutor",
          "forFailureTypes": ["validation_error", "internal_error"],
          "priority": 8
        }
      ]
    }
  ],
  "adaptiveToolConfigs": [
    {
      "toolName": "imageGenerator",
      "maxAdjustmentAttempts": 3,
      "parameterSensitivity": {
        "resolution": 0.8,
        "quality": 0.7,
        "timeout": 0.5,
        "batchSize": 0.9
      },
      "adjustmentStrategies": [
        {
          "parameterName": "timeout",
          "strategy": "timeout_adjustment",
          "minValue": 1000,
          "maxValue": 60000,
          "step": 5000
        },
        {
          "parameterName": "batchSize",
          "strategy": "numeric_scaling",
          "minValue": 1,
          "maxValue": 50
        },
        {
          "parameterName": "resolution",
          "strategy": "categorical_substitution",
          "options": ["low", "medium", "high"]
        }
      ]
    },
    {
      "toolName": "webSearch",
      "maxAdjustmentAttempts": 2,
      "parameterSensitivity": {
        "query": 0.9,
        "limit": 0.5,
        "filter": 0.7
      },
      "adjustmentStrategies": [
        {
          "parameterName": "query",
          "strategy": "query_reformulation"
        },
        {
          "parameterName": "limit",
          "strategy": "numeric_scaling",
          "minValue": 1,
          "maxValue": 50
        }
      ]
    }
  ]
} 